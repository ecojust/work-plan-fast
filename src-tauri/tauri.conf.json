{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "wpf",
  "version": "1.0.1",
  "identifier": "com.juzisang.wpf",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "wpf",
        "width": 800,
        "height": 600,
        "additionalBrowserArgs": "disable-web-security",
        "devtools": true,
        "alwaysOnTop": false,
        "fullscreen": false,
        "focus": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
